<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.playhaven.androidsdk"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
   	<uses-permission android:name="android.permission.INTERNET" />
	
    <application android:icon="@drawable/icon" android:label="@string/app_name"
    android:debuggable="true">
		<provider android:name="com.playhaven.src.prefetch.PHPrefetchLocalContentProvider"
          	android:authorities="com.playhaven.src.prefetch.PHPrefetchLocalContentProvider" />
        
        <activity android:name="com.playhaven.sampleapp.SampleApp"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.playhaven.sampleapp.PublisherOpenView"></activity>
        <activity android:name="com.playhaven.sampleapp.PublisherContentView"></activity>
		<activity android:name="com.playhaven.src.publishersdk.content.PHContentView" android:theme="@android:style/Theme.Dialog"></activity>
    </application>
</manifest>